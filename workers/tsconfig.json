{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "sourceMap": true,
        "skipLibCheck":true,
        "outDir": "Z:/www/fgd/schengenbot/dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/",
                "types/*"
            ]
        },
        "types":["node"],
        "lib": [
            "es5", "es6", "dom", "dom.iterable"
        ]
        
    },
    "include": [
        "src/**/*.ts",
        "*.ts",
    ]
}
